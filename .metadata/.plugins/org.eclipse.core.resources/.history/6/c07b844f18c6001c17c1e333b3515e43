package com.qa.demo;

public class StringManipulation {
public static void main(String[] args) {
	
	String  str1 = new String ("yesterday it was raining");
	String  str2 = new String ("today it is sunny");
	
	String mySubString1= str2.substring(0, 11);
	String mySubString2= str1.substring(15, 24);
	
	System.out.println(mySubString1 + mySubString2);
	
}
}
