package com.qa.demo;

public class revisitedcalc {

	public static void main(String[]args) {
		
		div2();
	
	}
	
	
		public static void div2() {
				int num1 = 20;
				int num2 = 10;
				int sum=(num1/num2);
				
		if (num1<num2)
				System.out.println(sum);
		
			 else {
			System.out.println("division cannot be performed");

		}
		
		
		}
		
}

