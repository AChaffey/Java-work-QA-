package com.qa.demo;

public class Blackjack {

	public static void main(String[] args) {

		blackJack(5, 17);
		System.out.println();
	}

	public static int blackJack(int num1, int num2) {
		
		if (num1>21 && num2>21) {
			return 0;	
	}	else {
		if (num1>num2) {
			return num2;
	} 	else {
			return num1;
	} 
		
		if (num2>num1) {
	}	else {
		if (num2>21) {
			return num1;
	}	else {
			return num2;
	}
	}
	}
	}
